{
  "problemMatcher": [
    {
      "owner": "rubocop-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\sC:\\s((.+):\\s.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    },
    {
      "owner": "rubocop-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.+):(\\d+):(\\d+):\\sW:\\s((.+):\\s.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    }
  ]
}
